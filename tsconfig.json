{
    "compilerOptions": {
      "target": "es6",
      "module": "es6",
      "moduleResolution": "node",
      "declaration": false,
      "noImplicitAny": false,
      "sourceMap": true,
      "noLib": false,
      "allowJs": true,
      "suppressImplicitAnyIndexErrors": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "ignoreDeprecations": "5.0",
    },
    "include": ["src/**/*"]
  }